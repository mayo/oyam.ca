<RoutingRules>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>rss/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyWith>feed/atom.xml</ReplaceKeyWith>
      <HttpRedirectCode>301</HttpRedirectCode>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>rss</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyWith>feed/atom.xml</ReplaceKeyWith>
      <HttpRedirectCode>301</HttpRedirectCode>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>articles/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <HostName>static.oyam.ca</HostName>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>archive/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <HostName>static.oyam.ca</HostName>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>archives/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <HostName>static.oyam.ca</HostName>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>past/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <HostName>static.oyam.ca</HostName>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>feed/blog/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyPrefixWith>blog/</ReplaceKeyPrefixWith>
      <HttpRedirectCode>301</HttpRedirectCode>
    </Redirect>
  </RoutingRule>
</RoutingRules>
